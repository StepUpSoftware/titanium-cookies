<project name="ticookies" default="both">
    <target name="ios">
        <exec dir="${basedir}/ios" executable="python" failonerror="true">
            <arg line="build.py"/>
        </exec>
    </target>
    <target name="android">
        <exec dir="${basedir}/android" executable="ant" failonerror="true">
        </exec>
    </target>
	<target name="both" depends="ios, android"/>
</project>
